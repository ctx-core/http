{
	"name": "@ctx-core/http",
	"version": "9.5.20",
	"description": "ctx-core http",
	"keywords": [
		"ctx-core",
		"http"
	],
	"homepage": "https://github.com/ctx-core/http#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/http/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/http.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./CacheControl_1hour_headers_",
		"./CacheControl_5min_headers_",
		"./ContentType__is_binary_",
		"./ContentType_json_headers_",
		"./ContentType_svg_headers_",
		"./ext_R_mime",
		"./http__headers__assign",
		"./internal_server_error__throw"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/object": "^31.1.2"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}